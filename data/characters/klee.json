  "constellations":{
    "1": {
      "name": "Explosión en cadena",
      "effect": "Al atacar o usar una habilidad, hay una probabilidad de invocar chispas que atacan al enemigo, las cuales infligen Daño equivalente a 120% del Daño de Chispas explosivas.",
      "img": "https://static.wikia.nocookie.net/gen-impact/images/0/0e/Constelaci%C3%B3n_Explosi%C3%B3n_en_cadena.png/revision/latest/scale-to-width-down/32?cb=20210123052212&path-prefix=es"
    },
    "2": {
      "name": "Fragmentos de metralla",
      "effect": "Las trampas cazabobos de Bomba saltarina reducen la DEF de los enemigos en 23% por 10 s.",
      "img": "https://static.wikia.nocookie.net/gen-impact/images/7/74/Constelaci%C3%B3n_Fragmentos_de_metralla.png/revision/latest/scale-to-width-down/32?cb=20210123052222&path-prefix=es"
    },
    "3": {
      "name": "Mezcla especial de Klee",
      "effect": "Aumenta el nivel de habilidad de Bomba saltarina +3. Puede ser aumentado hasta Niv. 15.",
      "img": "https://static.wikia.nocookie.net/gen-impact/images/8/8f/Constelaci%C3%B3n_Mezcla_especial_de_Klee.png/revision/latest/scale-to-width-down/32?cb=20210123052302&path-prefix=es"
    },
    "4": {
      "name": "¡Bomba va!",
      "effect": "Si el personaje se retira mientras la habilidad Chispas explosivas está activa, creará una explosión que inflige 555% de Daño Pyro de Área.",
      "img": "https://static.wikia.nocookie.net/gen-impact/images/2/20/Constelaci%C3%B3n_%C2%A1Bomba_va%21.png/revision/latest/scale-to-width-down/32?cb=20210123052402&path-prefix=es"
    },
    "5": {
      "name": "Estrella de los bombardeos",
      "effect": "Aumenta el nivel de habilidad de Chispas explosivas +3. Puede ser aumentado hasta Niv. 15.",
      "img": "https://static.wikia.nocookie.net/gen-impact/images/1/10/Constelaci%C3%B3n_Estrella_de_los_bombardeos.png/revision/latest/scale-to-width-down/32?cb=20210123052202&path-prefix=es"
    },
    "6": {
      "name": "¡A la carga!",
      "effect": "Mientras las Chispas explosivas estén activas, los miembros del equipo recuperarán energía elemental; después de activar Chispas explosivas, y por 25 s todos los miembros del equipo reciben 10% de Bono de Daño Pyro.",
      "img": "https://static.wikia.nocookie.net/gen-impact/images/0/0d/Constelaci%C3%B3n_%C2%A1A_la_carga%21.png/revision/latest/scale-to-width-down/32?cb=20210123052401&path-prefix=es"
    }
  }
